<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CountTimeButton">
        <attr name="ableClickBackground" format="reference|color"/>
        <attr name="disableClickBackground" format="reference|color"/>
        <attr name="totalTime" format="integer"/>
        <attr name="stepTime" format="integer"/>
    </declare-styleable>
    
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension"/>
        <attr name="civ_border_color" format="color"/>
        <attr name="civ_border_overlay" format="integer"/>
        <attr name="civ_fill_color" format="integer"/>
    </declare-styleable>

</resources>